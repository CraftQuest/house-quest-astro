export async function getEntry(query: string) {
    const url = 'http://localhost:8888/api';
    const response = await fetch (url, {
    method: "POST",
    headers: {
        "Content-Type": "application/json",
    },
    body: JSON.stringify({
        query: 
        query
        ,
    }),
});

let json = await response.json();
return json.data.entry;
}

